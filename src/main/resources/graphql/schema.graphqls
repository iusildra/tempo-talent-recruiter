type Query {
  countries: [Country]
  countryById(id: Int!): Country
  cities: [City]
  cityById(id: Int): City
  citiesByCountryId(countryId: Int, page: Int, size: Int): [City]
  registerCity(city: City): City
}

type Country {
  id: Int
  name: String
}

type City {
  id: Int
  name: String
  country: Country
}